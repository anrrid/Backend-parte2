{
  "name": "tercera-preentrega",
  "version": "1.0.0",
  "description": "```\r npm run start\r ```",
  "main": "auth.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:autocannon": "node ./src/performance/benchmark.js",
    "start:0x": "0x ./index.js",
    "start": "nodemon server.js",
    "ServerES6toJS5": ".\\node_modules\\.bin\\babel server.es6.js -o server.js",
    "start:fork": "nodemon server.js fork 8080",
    "start:cluster": "nodemon server.js cluster 8080",
    "prod:forever": "forever start -w index.js",
    "prod:pm2:fork": "pm2 start index.js --name='ServerFork' --watch",
    "prod:pm2:cluster": "pm2 start index.js --name='ServerCluster' --watch -i max",
    "start:prof": "node --prof index.js FORK 8080",
    "test:info:artillery": "artillery quick --count 50 -n 20 http://localhost:8080/info",
    "start:inspect": "node --inspect index.js"
  },
